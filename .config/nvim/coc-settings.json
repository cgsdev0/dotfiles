{
  "go.goplsPath": "/home/ubuntu/co/backend/bin/gopls",
  "go.goplsOptions": {
    "expandWorkspaceToModule": false,
    "staticcheck": false,
    "local": "samsaradev.io",
    "codelenses": {
      "gc_details": false,
      "generate": false,
      "test": false
    }
  },
  "tslint.autoFixOnSave": true,
  "languageserver": {
      "ccls": {
          "command": "ccls",
          "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
          "rootPatterns": [".ccls-root", "compile_commands.json"],
          "initializationOptions": {
              "cache": {
                  "directory": ".ccls-cache"
              },
              "client": {
                  "snippetSupport": true
              }
          }
      }
  },
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascript.jsx",
    "typescript.tsx",
    "json"
  ]
}
